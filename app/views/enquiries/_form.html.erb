<%= simple_form_for(@enquiry) do |f| %>
  <%= f.error_notification %>
    <%= invisible_captcha %>
  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :email %>
    <%= f.input :phone %>
    <%= f.input :message_type, collection: ['Website error', 'Startup question', 'General Enquiry'] %>
    <%= f.input :message %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
