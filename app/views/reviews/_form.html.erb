<%= simple_form_for(@review, wrapper: :inline_form,
  html: { class: 'form-inline' }) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    
    <%= f.input :startup_id, as: :hidden, input_html: { value: params[:startup]} %>
    <%= f.input :content %>
    <%= f.input :rating, collection: 1..5, as: :radio_buttons %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
